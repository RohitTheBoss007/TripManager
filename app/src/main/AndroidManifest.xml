<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.tripmanager">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.Dark">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignUp" />
        <activity android:name=".Final" />
        <activity android:name=".GroupChatActivity" />
        <activity android:name=".TripActivity" />
        <activity android:name=".FindFriends" />
        <activity android:name=".Places" />
        <activity android:name=".PicturesActivity" />
        <activity android:name=".AddPics" />
        <activity android:name=".AboutActivity" />
        <activity android:name=".ExpenseActivity" />
        <activity android:name=".AddExpense"></activity>
    </application>

</manifest>